{
  "name": "constrainodelaunato",
  "version": "1.0.0",
  "description": "Constrained Delaunay triangulator build off of the delaunator package.",
  "main": "constrainment.js",
  "module": "main.js",
  "jsdelivr": "constrainodelaunato.min.js",
  "unpkg": "constrainodelaunato.min.js",
  "sideEffects": false,
  "dependencies": {
    "delaunator": "^4.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.0.0",
    "browser-sync": "^2.26.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25",
    "rollup": "^1.29.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-terser": "^5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/willmac321/constrainodelaunato.git"
  },
  "scripts": {
    "lint": "eslint index.js test/test.js bench.js rollup.config.js docs/diagrams.js",
    "pretest": "npm run lint",
    "test": "node -r esm test/test.js",
    "bench": "node -r esm bench.js",
    "build": "rollup -c",
    "start": "browser-sync start --server --files './*.js' './main.html' --index \"main.html\" & rollup -cw",
    "prepublishOnly": "npm test && npm run build",
    "dev": "browser-sync start --server --files './*.js' './main.html' --index \"main.html\""
  },
  "files": [
    "constrainment.js",
    "boundary.js",
    "constrainodelaunato.js",
    "constrainodelaunato.min.js"
  ],
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "no-sequences": 0
    }
  },
  "keywords": [
    "delaunay triangulation",
    "computational geometry",
    "algorithms",
    "Ruppert's Algorithm",
    "constrained delaunay triangulation",
    "concave hull"
  ],
  "author": "Will MacIntyre",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/willmac321/constrainodelaunato/issues"
  },
  "homepage": "https://gitlab.com/willmac321/constrainodelaunato#readme"
}
